{
  "name": "gclone",
  "version": "1.0.0",
  "description": "Github clone",
  "main": "index.js",
  "scripts": {
    "start-server": "nodemon api/index.js --watch api --exec babel-node",
    "start-client": "webpack-dev-server -d --hot --inline --no-info --port 5000",
    "start": "concurrently \"npm run start-server\" \"npm run start-client\"",
    "test": "mocha --compilers js:babel-core/register --reporter spec --full-trace 'api/**/*.test.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shwetank30/gclone.git"
  },
  "author": "Shwetank Kumar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Shwetank30/gclone/issues"
  },
  "homepage": "https://github.com/Shwetank30/gclone#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "html-webpack-plugin": "^2.30.1",
    "mocha": "^4.0.1",
    "nodemon": "^1.12.1",
    "webpack": "^3.9.1",
    "webpack-dev-server": "^2.9.5"
  },
  "dependencies": {
    "apollo-client": "^2.1.0",
    "apollo-server": "^0.1.5",
    "dataloader": "^1.3.0",
    "express": "^4.16.2",
    "graphql": "^0.11.7",
    "knex": "^0.14.2",
    "lodash": "^4.17.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router": "^4.2.0",
    "redux": "^3.7.2",
    "request-promise": "^4.2.2",
    "sqlite3": "^3.1.13"
  }
}
